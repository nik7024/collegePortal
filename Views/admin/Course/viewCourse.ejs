    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
      <!-- Content Header (Page header) -->
      <div class="content-header">
        <div class="container-fluid">
          <div class="row mb-2">
            <div class="col-sm-6">
              <h1 class="m-0">Contact</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
              <ol class="breadcrumb float-sm-right">
                <li class="breadcrumb-item"><a href="#">Home</a></li>
                <li class="breadcrumb-item active">Course Display</li>
              </ol>
            </div><!-- /.col -->
          </div><!-- /.row -->
        </div><!-- /.container-fluid -->
      </div>
      <!-- /.content-header -->

      <!-- Main content -->
      <div class="content">
        <div class="container-fluid">
          <div class="row">
            <div class="col-12">
              <div class="card">
                <div class="card-header">
                  <h3 class="card-title">DataTable with default features</h3>
                </div>
                <!-- /.card-header -->
                <div class="card-body">
                  <table id="example1" class="table table-bordered table-striped">
                    <thead>
                      <tr>

                        <th>Name </th>
                        <th>Email</th>
                        <th>Course</th>
                        <th>phone</th>
                        <th>Address</th>
                        <th>Status</th>
                        <th>Comment</th>

                      </tr>
                    </thead>
                    <tbody>
                      <tr>

                        <td>
                          <%=c.name %>
                        </td>
                        <td>
                          <%=c.email %>
                        </td>
                        <td>
                          <%=c.course %>
                        </td>
                        <td>
                          <%=c.phone %>
                        </td>
                        <td>
                          <%=c.address%>
                        </td>
                        <td>
                          <%=c.status %>
                        </td>
                        <td>
                          <%=c.comment %>
                        </td>



                      </tr>


                    </tbody>



                  </table>
                </div>
                <!-- /.card-body -->
              </div>
              <!-- /.card -->
            </div>
            <!--card-col-end-->



          </div>
          <!-- /.row -->
        </div><!-- /.container-fluid -->
      </div>
      <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->

    <%-include("../footer.ejs")%>
      
    